                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="ContactData">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element maxOccurs="unbounded" name="Telefon">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute name="Type" type="xs:string" use="required" />
